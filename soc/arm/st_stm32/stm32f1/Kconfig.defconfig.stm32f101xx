config SOC
	default "stm32f101x4-6" if SOC_STM32F101X6
	default "stm32f101x8-b" if SOC_STM32F101XB
	default "stm32f101xc-d-e" if SOC_STM32F101XE
	default "stm32f101xf-g" if SOC_STM32F101XG

config NUM_IRQS
	default 60 if SOC_STM32F101X6
	default 60 if SOC_STM32F101XB
	default 60 if SOC_STM32F101XE
	default 60 if SOC_STM32F101XG